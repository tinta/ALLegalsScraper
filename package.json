{
  "name": "ALLegalsScraper",
  "version": "1.0.0",
  "description": "Scrape Public Notices from AlabamaLegals.com",
  "main": "index.js",
  "scripts": {
    "start": "/usr/local/bin/node app/index.js",
    "test": "./node_modules/.bin/jasmine-node --coffee --matchall --verbose ./spec",
    "scraper": "/usr/local/bin/node scraper/index.js",
    "lint": "node_modules/.bin/standard app/resources/js/*",
    "build:js": "node_modules/.bin/uglifyjs $(cat to-uglify.txt) -o app/public/build.js --source-map app/public/build.js.map",
    "build:css": "cat $(cat to-csso.txt) | node_modules/.bin/csso -o ./app/public/build.css -m ./app/public/build.css.map --stat",
    "build": "npm run build:js && npm run build:css"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "angular": "1.3.15",
    "angular-bootstrap-npm": "0.13.0",
    "angular-sanitize": "1.3.15",
    "body-parser": "1.12.3",
    "bootstrap": "3.3.4",
    "client-sessions": "0.7.0",
    "cookie-parser": "1.3.4",
    "express": "4.12.3",
    "express-session": "1.11.2",
    "font-awesome": "4.3.0",
    "foundation": "4.2.1-1",
    "googleapis": "2.0.3",
    "jade": "1.9.2",
    "jquery": "2.1.3",
    "lodash": "3.6.0",
    "method-override": "2.3.3",
    "moment": "2.10.2",
    "mysql": "2.6.2",
    "ng-table": "0.5.4",
    "nightmare": "1.8.0",
    "passport": "0.2.1",
    "passport-google-oauth": "0.2.0",
    "q": "1.3.0",
    "squel": "3.10.0"
  },
  "devDependencies": {
    "csso": "^1.6.4",
    "jasmine-node": "1.14.5",
    "standard": "^4.0.1",
    "uglify-js": "^2.6.2"
  }
}
