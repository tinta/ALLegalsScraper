h4.text-left &nbsp;Home History
div.spacer-xxs

table.table.table-bordered
    thead
        tr
            td
                label.editable(for="name1")
                    span Name 1
                    i.fa.fa-pencil

            td
                label.editable(for="name2")
                    span Name 2
                    i.fa.fa-pencil

            td
                label.editable(for="build_year")
                    span Year Built
                    i.fa.fa-pencil

            td
                label.editable(for="last_sold_price")
                    span Last Sold Price
                    i.fa.fa-pencil
            td
                label.editable(for="last_sold_year")
                    span Last Sold Year
                    i.fa.fa-pencil
    tbody
        tr
            +editableTD('name1', 10)
            +editableTD('name2', 11)
            +editableTD('build_year', 12)
            +editableTD('last_sold_price', 13)
            +editableTD('last_sold_year', 14)