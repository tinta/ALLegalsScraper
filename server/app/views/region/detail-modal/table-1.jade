table.table.table-bordered
    thead
        tr
            td County
            td
                label.editable(for="city")
                    span City
                    i.fa.fa-pencil
            td
                label.editable(for="zip")
                    span Zip
                    i.fa.fa-pencil
            td
                label.editable(for="street_addr")
                    span Address
                    i.fa.fa-pencil

    tbody
        tr
            td [[ modal.data.model.county ]]

            +editableTD('city', 1)
            +editableTD('zip', 2)
            +editableTD('street_addr', 3)