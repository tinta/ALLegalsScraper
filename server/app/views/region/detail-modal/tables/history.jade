table.table.table-bordered
    thead
        tr
            td
                label.editable(for="name1")
                    i.fa.fa-pencil
                    i.fa.fa-user
                    span Name 1

            td
                label.editable(for="name2")
                    i.fa.fa-pencil
                    i.fa.fa-user-plus
                    span Name 2

            td
                label.editable(for="build_year")
                    i.fa.fa-pencil
                    i.fa.fa-calendar-o
                    span Year Built

            td
                label.editable(for="last_sold_price")
                    i.fa.fa-pencil
                    i.fa.fa-dollar
                    span Last Sold Price

    tbody
        tr
            +editableTD('name1', 11)
            +editableTD('name2', 12)
            +editableTD('build_year', 13)
            +editableTD('last_sold_price', 14)
